<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adilson.airlinetom">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/travel"
        android:label="@string/app_name"
        android:roundIcon="@drawable/travel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrimeiraActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".PesquisaActivity" />
        <activity android:name=".VooActivity" />
        <activity android:name=".PoltronaActivity" />
        <activity android:name=".ListaPassageActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CartaoActivity" />
        <activity android:name=".RecusadoActivity" />
        <activity android:name=".AprovadoActivity"></activity>
    </application>

</manifest><!--
